
.table{
    border-radius: 20px;
    text-align: center;
    float: left;
    margin-right: 10px;
    margin-top: 5px;
    background-color: #fff;
}
.table1{
    vertical-align: middle;
    display: inline-block;
    border-radius: 20px;
    border: 2px dotted #eec82e;
    text-align: center;
    margin-top: -3px;
}
.table1 text{
    font-size: 14px;
}
.tableWrap .table:nth-child(3){
    margin-right: 0;
}