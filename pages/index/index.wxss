page {
    background-color: #fff;
}
.listWrap{
    color: #484848;
}
.listWrap .tableWrap:nth-child(odd) {
    border-right: 1rpx solid #eee;
}

.tableWrap {
    float: left;
    border-bottom: 1rpx solid #eee;
}
.tableImgWrap{
    text-align:center;margin-top:10px;
}
.tableImg{
    width:70px;height:64px;
}
.tableWrap .contentWrap{
    overflow:hidden;padding-left:20px;padding-right:20px;margin-bottom:10px;
}
.box {
background-color:#fff;
margin-top:0.375rem;
}
.list-hd {
display:flex;
line-height:36px;
text-align:center;
}
.has-hairline {
position:relative;
}
.hairline-in-pseudo-bottom:after {
content:" ";
position:absolute;
left:0;
bottom:0;
width:100%;
height:0;
transform:scaleY(0.5);
-webkit-transform:scaleY(0.5);
border-bottom:1px solid rgba(0,0,0,.1);
}
.list-hd .item.current {
color:#EEC82E;
}
.list-hd .item {
width:33%;
color:#6b6b6b;
font-size:14px;
position:relative;
}
.list-hd .item.current:after {
display:inline;
}
.list-hd .item:after {
display:none;
content:" ";
position:absolute;
left:50%;
bottom:0;
width:75%;
height:1.5px;
margin-left:-37.5%;
background-color:#EEC82E;

}

